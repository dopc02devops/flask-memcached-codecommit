{
  "pipeline": {
    "name": "MyPipeline",
    "roleArn": "arn:aws:iam::908027384199:role/CodePipelineRole",
    "artifactStore": {
      "type": "S3",
      "location": "flask-memcached-app-bucket"
    },
    "stages": [
      {
        "name": "Source",
        "actions": [
          {
            "name": "SourceAction",
            "actionTypeId": {
              "category": "Source",
              "owner": "ThirdParty",
              "provider": "GitHub",
              "version": "1"
            },
            "runOrder": 1,
            "configuration": {
              "Owner": "dopc02devops",
              "Repo": "flask-memcached-codecommit",
              "Branch": "main",
              "OAuthToken": ""
            },
            "outputArtifacts": [
              {
                "name": "SourceOutput"
              }
            ],
            "region": "eu-west-2"
          }
        ]
      },
      {
        "name": "Build",
        "actions": [
          {
            "name": "BuildAction",
            "actionTypeId": {
              "category": "Build",
              "owner": "AWS",
              "provider": "CodeBuild",
              "version": "1"
            },
            "runOrder": 1,
            "configuration": {
              "ProjectName": "flask-memcached-build-Project"
            },
            "inputArtifacts": [
              {
                "name": "SourceOutput"
              }
            ],
            "outputArtifacts": [
              {
                "name": "BuildOutput"
              }
            ],
            "region": "eu-west-2"
          }
        ]
      },
      {
        "name": "Test",
        "actions": [
          {
            "name": "TestAction",
            "actionTypeId": {
              "category": "Build",
              "owner": "AWS",
              "provider": "CodeBuild",
              "version": "1"
            },
            "runOrder": 1,
            "configuration": {
              "ProjectName": "flask-memcached-test-Project"
            },
            "inputArtifacts": [
              {
                "name": "BuildOutput"
              }
            ],
            "outputArtifacts": [
              {
                "name": "TestOutput"
              }
            ],
            "region": "eu-west-2"
          }
        ]
      },
      {
        "name": "Approval",
        "actions": [
          {
            "name": "ManualApproval",
            "actionTypeId": {
              "category": "Approval",
              "owner": "AWS",
              "provider": "Manual",
              "version": "1"
            },
            "runOrder": 1,
            "configuration": {
              "CustomData": "Please review the pipeline before deploying."
            },
            "region": "eu-west-2"
          }
        ]
      },
      {
        "name": "Deploy",
        "actions": [
          {
            "name": "DeployAction",
            "actionTypeId": {
              "category": "Deploy",
              "owner": "AWS",
              "provider": "CodeDeploy",
              "version": "1"
            },
            "runOrder": 1,
            "configuration": {
              "ApplicationName": "flask-memcached-app",
              "DeploymentGroupName": "flask-memcached-Deploy-Group"
            },
            "inputArtifacts": [
              {
                "name": "BuildOutput"
              },
              {
                "name": "TestOutput"
              }
            ],
            "region": "eu-west-2"
          }
        ]
      }
    ]
  }
}
